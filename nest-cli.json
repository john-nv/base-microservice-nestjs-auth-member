{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/admin-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/admin-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/admin-gateway",
  "projects": {
    "admin-gateway": {
      "type": "application",
      "root": "apps/admin-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/admin-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "config-service": {
      "type": "application",
      "root": "apps/config-service",
      "entryFile": "main",
      "sourceRoot": "apps/config-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/config-service/tsconfig.app.json"
      }
    },
    "member-service": {
      "type": "application",
      "root": "apps/member-service",
      "entryFile": "main",
      "sourceRoot": "apps/member-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/member-service/tsconfig.app.json"
      }
    },
    "communicate-service": {
      "type": "application",
      "root": "apps/communicate-service",
      "entryFile": "main",
      "sourceRoot": "apps/communicate-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/communicate-service/tsconfig.app.json"
      }
    }
  }
}